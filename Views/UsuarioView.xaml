<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BLOGSOCIALUDLA.Views.UsuarioView"
             Title="Perfil de Usuario">

    <StackLayout Padding="20">
        <Label Text="Perfil de Usuario" FontSize="24" HorizontalOptions="Center" />
        <Label Text="Nombres:" FontSize="18" />
        <Label x:Name="NombresLabel" FontSize="18" />
        <Label Text="Apellidos:" FontSize="18" />
        <Label x:Name="ApellidosLabel" FontSize="18" />
        <Label Text="Número de Teléfono:" FontSize="18" />
        <Label x:Name="NumeroTelefonoLabel" FontSize="18" />
        <Label Text="Correo:" FontSize="18" />
        <Label x:Name="CorreoLabel" FontSize="18" />
    </StackLayout>

</ContentPage>
